
//BISMILLAHIR RAHMANIR RAHIM

#include<bits/stdc++.h>
using namespace std;
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define  endl '\n'
int main()
{
    optimize();

    map<int,int> cnt;
    int n,i;
    cin>>n;

    for(i=1 ;i<=n;i++)
    {
        int x;
        cin>>x;

        cnt[x]++;

    }

    int ans =0;

    for(auto u:cnt)
    {
        if(u.second>=u.first) ans+= (u.second - u.first);
        else ans += u.second;
    }

    cout<<ans<<endl;


    return 0;
}
