


#include<bits/stdc++.h>
using namespace std;
int main(){


    int n=4,a=0,b=0,c=0,d=0;

    while(n--){
     string s;
     cin>>s;
     if(s=="H"){
     	a++;
     }
     if(s=="2B"){
     	b++;
     }
     if(s=="3B"){
     	c++;
     }
     if(s=="HR"){
     	d++;
     }

    }
   if(a==1 && b==1 && c==1 && d==1){
   	cout<<"Yes"<<endl;
   }
   else {
   	cout<<"No"<<endl;
   }
   return 0;
}

