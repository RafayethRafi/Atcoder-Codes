https://atcoder.jp/contests/abc216/tasks/abc216_a

#include<bits/stdc++.h>
using namespace std;
#define endl '\n'
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
int main()
{
    optimize();
    double x;
    cin>>x;
    int y =x;
    double z = x-y;
    int p = z*10;
    if(p>=0 && p<=2)cout<<y<<"-"<<endl;
    else if(p>=3 && p<=6) cout<<y<<endl;
    else if(p>=7 && p<=9) cout<<y<<"+"<<endl;


    return 0;
}
